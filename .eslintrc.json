{
	"env": {
		"browser": true,
		"commonjs": true,
		"mocha": true,
		"node": true
	},
	"extends": "airbnb-base",
	"rules": {
		"indent": [
			"error",
			"tab"
		],
		"linebreak-style": [
			"error",
			"unix"
		],
		"quotes": [				
			"error",
			"single"
		],
		"semi": [
			"error",
			"never"
		]
	}
}